{
    "manifest_version": 2,
    "name": "crazyClicker",
    "description": "Click anywhere, anytime. Count your clicks. Waste your bored time :).",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "ui.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/mani.js"]
    }],
    "permissions": [
        "activeTab"
    ]
}